#include<stdio.h>




//记录一个日期是该年度的第几天
int main(void)
{
	int year, month, day;
	scanf("%d,%d,%d", &year, &month, &day);
	//是否为闰年？
	int R = 0;
	if (year % 400 == 0)R = 1;
	else if ((year % 4 == 0 && year % 100 != 0))R = 1;

	//记录每个月的天数
	char Mon[] = { 31,28,31,30,31,30,31,31,30,31,30,31 };
	int i, sum = 0;
	for (i = 0; i < month - 1; i++)
	{
		sum = sum + Mon[i];
	}
	if (month>2&&R == 1)sum += 1;

	//加上当月的天数
	printf("%d/%d/%d, 是当年的第%d天\n", year, month, day, sum + day);
return 0;
}
